# Frontend Firebase Configuration (Firebase Console -> Project settings -> Your apps -> Web app)
VITE_GROK_API_KEY=your_grok_api_key
VITE_GROK_MODEL=grok-2-latest
VITE_GROK_VISION_MODEL=grok-2-vision-latest
VITE_USE_GROK=true
VITE_AI_PROVIDER=grok
# Optional legacy fallback (Groq). Keep empty unless using Groq:
# VITE_GROQ_API_KEY=your_groq_api_key

VITE_FIREBASE_API_KEY=your_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id
VITE_FIREBASE_FUNCTIONS_REGION=us-central1

# Optional fallback if you still use EmailJS elsewhere in app
VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_TEMPLATE_ID=your_template_id
VITE_EMAILJS_PUBLIC_KEY=your_public_key
